{
  "name": "LockIn",
  "description": "Blocks distracting websites and makes you solve a flashcard set before you can access the website.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/lock.png",
      "32": "icons/lock.png",
      "48": "icons/lock.png",
      "128": "icons/lock.png"
    }
  },
  "icons": {
    "16": "icons/lock.png",
    "32": "icons/lock.png",
    "48": "icons/lock.png",
    "128": "icons/lock.png"
  }
}
